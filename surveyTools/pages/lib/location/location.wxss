/* pages/lib/location/location.wxss */
.page_hd{}
.page_hd .page_title{
  color: #ccc;
  border-bottom: 1rpx solid #666;
}
.home_page{
  width: 100%;
  height: 100%;
  background-color: #202A38;
}
image{
  width: 100%;
  height: 100%;
}
image{display: block;}
.cssBox {
  width: 360rpx;
  height: 360rpx;
  background-color: #202A38;
  position: fixed;
  margin-top: -180rpx;
  margin-left: -180rpx;
  top: 50%;
  left: 50%;
}
.cssBox .borderRadius {
  width: 276rpx;
  height: 276rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -138rpx 0 0 -138rpx;
  -webkit-animation: rotating 2s ease 0s infinite;
  animation: rotating 2s ease 0s infinite;
}
@-webkit-keyframes rotating {
  0% {
      -webkit-transform: rotate(0deg);
      transform: rotate(0deg);
      opacity: 1;
  }
  65% {
      -webkit-transform: rotate(90deg);
      transform: rotate(90deg);
      opacity: 1;
  }
  100% {
      -webkit-transform: rotate(90deg);
      transform: rotate(90deg);
      opacity: 0;
  }
}
.cssBox .borderRadius .lfRaidus {
  width: 276rpx;
  height: 138rpx;
  border-radius: 138rpx 138rpx 0 0;
  position: relative;
  -webkit-animation: lfRaidus 2s ease 0s infinite;
  animation: lfRaidus 2s ease 0s infinite;
}
@-webkit-keyframes lfRaidus {
  0% {
      background: -moz-linear-gradient(right,red,black);background: -webkit-linear-gradient(right,#00869c,#202A38);background: -o-linear-gradient(right,red,black);
  }
  65% {
      background: -moz-linear-gradient(right,red,black);background: -webkit-linear-gradient(right,#00869c,#202A38);background: -o-linear-gradient(right,red,black);
  }
  100% {
      background: -moz-linear-gradient(right,red,black);background: -webkit-linear-gradient(right,#00869c,#202A38);background: -o-linear-gradient(right,red,black);
  }
}
.cssBox .borderRadius .lfRaidus .ctRaidus {
  width: 262rpx;
  height: 131rpx;
  background-color: #202A38;
  border-radius: 131rpx 131rpx 0 0;
  position: absolute;
  left: 50%;bottom: 0;
  -webkit-transform: translate(-50%, 0);
  transform: translate(-50%, 0);
}
.cssBox .borderRadius .rtRaidus {
  width: 276rpx;
  height: 138rpx;
  border-radius: 0 0 138rpx 138rpx;
  position: relative;
  -webkit-animation: rtRaidus 2s ease 0s infinite;
  animation: rtRaidus 2s ease 0s infinite;
}
@-webkit-keyframes rtRaidus {
  0% {
      background: -moz-linear-gradient(right,red,black);background: -webkit-linear-gradient(right,#0F956F,#00869c);background: -o-linear-gradient(right,red,black);
  }
  65% {
      background: -moz-linear-gradient(right,red,black);background: -webkit-linear-gradient(right,#0F956F,#00869c);background: -o-linear-gradient(right,red,black);
  }
  100% {
      background: -moz-linear-gradient(right,red,black);background: -webkit-linear-gradient(right,#0F956F,#00869c);background: -o-linear-gradient(right,red,black);
  }
}
.cssBox .borderRadius .rtRaidus .ctRaidus {
  width: 262rpx;
  height: 131rpx;
  background-color: #202A38;
  border-radius: 0 0 131rpx 131rpx;
  position: absolute;
  left: 50%;
  top: 0;
  -webkit-transform: translate(-50%, 0);
  transform: translate(-50%, 0);
}
.cssBox .borderRadius .rtRaidus .hfPoint {
  width: 7rpx;
  height: 5rpx;
  position: absolute;
  left: 0rpx;
  top: -5rpx;
  border-radius: 5rpx 5rpx 0 0;
  background-color: #00869c;
}
.cssBox .borderRadius .rtRaidus .rtPoint {
  width: 5rpx;
  height: 5rpx;
  position: absolute;
  left: 10rpx;
  top: -5rpx;
  border-radius: 5rpx;
  background-color: #00869c;
}
.cssBox .imageRadius {
  margin-top: 4rpx;
  opacity: 0;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  -webkit-animation: imageRadius 2s ease 0s infinite;
  animation: imageRadius 2s ease 0s infinite;
}
@-webkit-keyframes imageRadius {
    0% {
        opacity: 0;
    }
    65% {
        opacity: 0;
    }
    100% {
        opacity: 1;
    }
}
.cssBox .waveRaidus {
  margin-top: 4rpx;
  opacity: 0;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  -webkit-animation: waveRaidus 2s ease-out 0s infinite;
  animation: waveRaidus 2s ease-out 0s infinite;
}
@-webkit-keyframes waveRaidus {
    0% {
        width: 100%;
        height: 100%;
        opacity: 0;
    }
    65% {
        width: 100%;
        height: 100%;
        opacity: 0;
    }
    66% {
        width: 100%;
        height: 100%;
        opacity: 0.8;
    }
    100% {
        width: 130%;
        height: 130%;
        opacity: 0;
    }
}
.cssBox .pointRadius {
  width: 10rpx;
  height: 10rpx;
  border-radius: 360rpx;
  background-color: #FFF;
  border: 1rpx solid transparent;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  -webkit-animation: pointRadius 2s ease 0s infinite;
  animation: pointRadius 2s ease 0s infinite;
}
@-webkit-keyframes pointRadius {
    0% {width: 10rpx;height: 10rpx;border: 0;box-shadow: 0 0 0rpx 3rpx rgba(255,255,255,0.3), 0 0 0rpx 6rpx rgba(255,255,255,0.2), 0 0 0rpx 9rpx rgba(255,255,255,0.1);}
    5% {width: 10rpx;height: 10rpx;border: 0;box-shadow: 0 0 0rpx 3rpx rgba(255,255,255,0.3), 0 0 0rpx 6rpx rgba(255,255,255,0.2), 0 0 0rpx 9rpx rgba(255,255,255,0.1);}
    10% {width: 12rpx;height: 12rpx;border: 0;box-shadow: 0 0 0rpx 2rpx rgba(255,255,255,0.3), 0 0 0rpx 4rpx rgba(255,255,255,0.2), 0 0 0rpx 6rpx rgba(255,255,255,0.1);}
    15% {width: 16rpx;height: 16rpx;border: 0;box-shadow: 0 0 0rpx 1rpx rgba(255,255,255,0.3), 0 0 0rpx 2rpx rgba(255,255,255,0.2), 0 0 0rpx 3rpx rgba(255,255,255,0.1);}
    20% {width: 26rpx;height: 26rpx;border: 0;box-shadow: none;}
    65% {width: 205rpx;height: 205rpx;border: 2rpx solid #CCC;}
    100% {width: 205rpx;height: 205rpx;border: 2rpx solid #CCC;}
}